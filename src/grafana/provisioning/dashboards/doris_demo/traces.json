          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "with a as (\r\nselect trace_id, min(timestamp) as t\r\n  from otel_traces\r\n  where $__timeFilter(timestamp)\r\n    and service_name = '$service_name'\r\n    and ($operation = 'all' or span_name = $operation)\r\n    and ($status = 'all' or status_code = $status)\r\n  group by trace_id\r\n  order by t desc\r\n  limit 1000\r\n)\r\nselect\r\n    unix_timestamp(min(timestamp)) as time,\r\n    trace_id,\r\n    collect_set(service_name) as services,\r\n    count(*) as spans,\r\n    sum(if('STATUS_CODE_ERROR' = status_code, 1, 0)) as error_spans,\r\n    max(duration) / 1000 as 'duration (ms)'\r\nfrom otel_traces\r\nwhere trace_id in (select trace_id from a)\r\ngroup by trace_id\r\nORDER BY time desc\r\n;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
