          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "with a as (\r\nselect\r\n  $__timeGroup(timestamp, '$interval') as time,\r\n  array_map((x, y) -> x - y, max_foreach(bucket_counts), min_foreach(bucket_counts)) as buckets,\r\n  any_value_foreach(explicit_bounds) as bounds,\r\n  cast(attributes as string) attr\r\nfrom otel_metrics_histogram\r\nwhere metric_name = 'rpc.client.duration'\r\n and $__timeFilter(timestamp)\r\nGROUP BY attr, time\r\nORDER BY time, attr desc\r\nLIMIT 10000\r\n)\r\nSELECT\r\n  time,\r\n  concat('{', array_join(array_map((x, y) -> concat(json_quote(y), ':', x), sum_foreach(buckets), array_pushback(cast(any_value_foreach(bounds) as array<string>), '+inf')), ','), '}') as buckets\r\n  from a\r\nGROUP BY time\r\nORDER BY time desc\r\n;\r\n",
          "refId": "A",
          "sql": {
